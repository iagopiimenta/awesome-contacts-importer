<div class="container">
  <div class="row">
    <div class="col">
      <h1>Import #<%= @contact_import.id %></h1>
    </div>
  </div>
  <div class="row mt-3">
    <div class="col">
      <dl>
        <dt>Status</dt>
        <dd><%= @contact_import.status %></dd>
      </dl>
    </div>
    <div class="col">
      <dl>
        <dt>Total imported with success</dt>
        <dd><%= link_to(@contact_import.contacts.count, contact_import_contacts_path(@contact_import)) %></dd>
      </dl>
    </div>
    <div class="col">
      <dl>
        <dt>Total imported with failure</dt>
        <dd><%= link_to(@contact_import.contact_faileds.count, contact_import_contact_faileds_path(@contact_import)) %></dd>
      </dl>
    </div>
  </div>
</div>
